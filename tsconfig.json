{
    "compilerOptions": {
      "target": "es5",
      "module": "es2015",
      "moduleResolution": "node",
      "lib": [
        "dom",
        "es5",
        "es2015"
      ],
      "esModuleInterop": true,
      "experimentalDecorators": true,
      "allowJs": true,
      "sourceMap": true,
      // "strict": true,
      "noEmit": true,
      "baseUrl": ".",
      "paths": {
        "~/*": [
          "./*"
        ],
        "@/*": [
          "./*"
        ]
      },
      "types": [
        "@types/node",
        "@nuxt/types",
        "@nuxtjs/axios",
        "nuxt-i18n"
      ]
    },
    "exclude": [
      "node_modules"
    ]
}